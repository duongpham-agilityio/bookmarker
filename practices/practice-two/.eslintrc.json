{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["error"],
    "quotes": ["error", "single"],
    "no-unused-vars": ["error"],
    "react/react-in-jsx-scope": "off",
    "@typescript-eslint/no-empty-function": "off"
  }
}
